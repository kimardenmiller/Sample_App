<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>

<body>

<%= render 'layouts/header' %>

<div class="container">
  <% flash.each do |key, value| %>
      <%= content_tag(:div, value, class: "alert alert-#{key}") %>
  <% end %>

      <!--<%= link_to "Test Link", sessions_path(username: "kimardenmiller@gmail.com", password: "lockup99"), method: "create", class: "btn btn-large btn-primary" %>-->

  <!--<%= link_to "Test 2", sessions_path(:method => "directin"), method: "directin", class: "btn btn-large btn-primary" %>-->




  <%= yield %>

  <%= render 'layouts/footer' %>
  <%= debug(params) if Rails.env.development? %>
</div>

</body>

</html>
